{
    "name": "fabianosfbr/header-select",
    "description": "This is my package header-select",
    "keywords": [
        "fabianosfbr",
        "laravel",
        "header-select"
    ],
    "homepage": "https://github.com/fabianosfbr/header-select",
    "support": {
        "issues": "https://github.com/fabianosfbr/header-select/issues",
        "source": "https://github.com/fabianosfbr/header-select"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Fabiano Fernandes",
            "email": "contato@fabianofernandes.adm.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^5.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Fabianosfbr\\HeaderSelect\\": "src/",
            "Fabianosfbr\\HeaderSelect\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fabianosfbr\\HeaderSelect\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fabianosfbr\\HeaderSelect\\HeaderSelectServiceProvider"
            ],
            "aliases": {
                "HeaderSelect": "Fabianosfbr\\HeaderSelect\\Facades\\HeaderSelect"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}